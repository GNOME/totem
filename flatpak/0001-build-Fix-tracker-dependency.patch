From f1ff48261d0e7a507682e64a2537fdd072f62de0 Mon Sep 17 00:00:00 2001
From: Bastien Nocera <hadess@hadess.net>
Date: Tue, 11 Jul 2023 12:56:58 +0200
Subject: [PATCH] build: Fix tracker dependency

Dependency tracker-sparql-3.0 found: NO found 3.6.alpha but need: '>=3.6.0'
---
 meson.build | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/meson.build b/meson.build
index 32409f44305e..0567ce0680d0 100644
--- a/meson.build
+++ b/meson.build
@@ -15,7 +15,7 @@ libdir = join_paths(get_option('prefix'), get_option('libdir'))
 datadir = join_paths(get_option('prefix'), get_option('datadir'))
 
 glib_required = '2.62.0'
-tracker_required = '3.6.0'
+tracker_required = '3.6'
 
 if get_option('tracker_core') == 'system'
   tracker_sparql = dependency('tracker-sparql-3.0', version: '>=' + tracker_required, required: false)
